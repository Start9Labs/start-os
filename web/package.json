    "@types/estree": "^0.0.51",
    "@types/js-yaml": "^4.0.5",
    "@types/marked": "^4.0.3",
    "@types/mustache": "^4.1.2",
    "@types/node": "^18.15.0",
    "@types/node-jose": "^1.1.10",
    "@types/pbkdf2": "^3.1.0",
    "@types/uuid": "^8.3.1",
    "autoprefixer": "^10.4.14",
